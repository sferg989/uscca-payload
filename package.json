{
  "name": "uscca-payload",
  "description": "Uscca Payload CMS",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "test": "jest --watch",
    "test-once": "jest --passWithNoTests --collectCoverage --coverageReporters lcov clover",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "lint-fix": "eslint ./src --ext .ts,.tsx --fix "
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@payloadcms/plugin-form-builder": "^1.0.2",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-seo": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "copyfiles": "^2.4.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.3.0",
    "node-cron": "^3.0.0",
    "payload": "^1.1.26",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/jasmine": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "@types/jest": "^26.0.23",
    "jasmine-core": "^4.3.0",
    "@testing-library/react": "^11.2.2",
    "enzyme": "^3.11.0",
    "@types/enzyme": "^3.10.8",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^5.4.5",
    "ts-node": "^10.9.1",
    "tsbase": "^4.7.1",
    "tslib": "^2.4.0",
    "tsmockit": "^1.3.0",
    "@types/express": "^4.17.9",
    "@types/node-cron": "^3.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-enforce-ids-in-jsx": "^1.0.0",
    "eslint-plugin-react": "^7.21.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "prettier": "^2.5.1",
    "typescript": "^4.1.3"
  }
}
