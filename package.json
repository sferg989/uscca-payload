{
  "name": "payload-public-demo",
  "description": "Payload Demo",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "lint-fix": "eslint ./src --ext .ts,.tsx --fix "
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@payloadcms/plugin-form-builder": "^1.0.2",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-seo": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-enforce-ids-in-jsx": "^1.0.0",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.5.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^4.3.0",
    "node-cron": "^3.0.0",
    "payload": "^1.1.26",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node-cron": "^3.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
